{
  "name": "dev-components",
  "version": "0.1.0",
  "private": false,
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "main": "dist/index",
  "dependencies": {
    "@babel/cli": "^7.23.9",
    "@heroicons/react": "^1.0.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.80",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "@mantine/core": "^7.6.2",
    "@mantine/hooks": "^7.6.2",
    "@storybook/preview-api": "^8.0.2",
    "@tabler/icons-react": "^3.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "watch:css": "npx tailwindcss -i ./src/index.css -o ./src/App.css ./src/components/tailwind.css --watch",
    "tailwind-refresh": "npx tailwindcss -i ./src/index.css -o ./src/App.css ./src/components/tailwind.css",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "delete-old": "del dist",
    "publish:npm": "babel src/Components --extensions \".js,.jsx,.ts,.tsx\" -d dist --copy-files"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.23.6",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@storybook/addon-a11y": "^7.6.14",
    "@storybook/addon-essentials": "^7.6.14",
    "@storybook/addon-interactions": "^7.6.14",
    "@storybook/addon-jest": "^7.6.14",
    "@storybook/addon-links": "^7.6.14",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.14",
    "@storybook/jest": "^0.2.3",
    "@storybook/preset-create-react-app": "^7.6.14",
    "@storybook/react": "^7.6.14",
    "@storybook/react-webpack5": "^7.6.14",
    "@storybook/test": "^7.6.14",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "babel-jest": "^29.7.0",
    "eslint-plugin-storybook": "^0.6.15",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prop-types": "^15.8.1",
    "react-test-renderer": "^18.2.0",
    "storybook": "^7.6.14",
    "tailwindcss": "^3.4.1",
    "webpack": "^5.90.1"
  }
}
